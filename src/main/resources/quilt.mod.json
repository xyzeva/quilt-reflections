		"id": "quilt_reflections",
		"version": "${version}",
		"metadata": {
			"name": "Quilt Reflections",
			"description": "A fix to use Reflections (https://github.com/ronmamo/reflections) on Quilt file systems.",
			"contributors": {
				"xyzeva": "Owner"
			},
			"contact": {
				"homepage": "https://modrinth.com/quilt-reflections/",
